<?xml version="1.1" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wills.zchsclubs">

    <application
        android:allowBackup="true"
        android:versionCode="3"
        android:icon="@drawable/eaglelogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eaglelogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListScroll"
            android:label="@string/app_name_1">
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.zchsclubs.MainActivity" >
        </activity>
        <activity
            android:name=".DisplayResults"
            android:label="@string/app_name_2">
            android:screenOrientation="portrait"
            android:parentActivityName="com.example.zchsclubs.MainActivity" >
        </activity>

    </application>

</manifest>